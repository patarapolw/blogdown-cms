    "fastify-oas": "^2.5.2",
    "fastify-static": "^2.6.0",
    "fs-extra": "^8.1.0",
    "mongoose": "^5.8.9",
    "nanoid": "^2.1.10",
    "pino-pretty": "^3.6.1",
